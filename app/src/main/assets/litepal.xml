<?xml version="1.0" encoding="utf-8"?>
<litepal>
    <!--数据库名-->
    <dbname value="Weather" />
    <!--数据库版本-->
    <version value="1" />
    <!--配置要映射（对象关系映射ORM）的模型类-->
    <list>
        <mapping class="com.example.myweather.db.Province"></mapping>
        <mapping class="com.example.myweather.db.City"></mapping>
        <mapping class="com.example.myweather.db.County"></mapping>
    </list>

</litepal>